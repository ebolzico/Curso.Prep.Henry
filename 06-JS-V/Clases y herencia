var bipedo(pies, dedos, cabezas) = {
	pies,
	dedos,
	cabezas,
	cuerpo = {
		torsos,
		musculos,
	}
	//saludo = function(){
        console.log ()
        }
}

var persona (pies, dedos, cabezas, nombre, apellido) = {
	bipedo.call(this.pies, this.dedos, this.cabezas)
	//pies
	//dedos
	//cabezas
	nombre,
	apellido,
	asdklnassdk;sa
}

let ema = new persona(2, 10, 2, 'juan', 'perez');

//ema es un objeto del tipo persona
//que hereda tambien las propiedades de la clase bipedo

console.log(ema.nombre);
//juan

console.log (ema.dedos);
//10

