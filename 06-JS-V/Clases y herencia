var clase = {
    nombre,
    edad,
    ciudad
    //saludo = 
}
clase.assign alumno1 = {this.nombre = emanuel, this.edad = 25, this.ciudad = caba}

clase.prototype.saludo = function(){
    return ('Hola, ' + this.nombre + '!')
}

object.assign ==> agregar objeto a una clase
let nombreObjeto = new clase (valores)
var nombreObjeto = object.create ({}) = var nombre = {}

var clase = {
    nombre = {alumno, profesor},
    edad,
    ciudad,
    saludo = function(){
    return ('Hola, ' + this.nombre + '!')
    }    
}

var clase2 = {
    clase.call (this),
    desayuno,
    merienda
}

var toni = new clase2 ('toni', 25, 'tucuman', 'cafe con leche', 'tostadas')


